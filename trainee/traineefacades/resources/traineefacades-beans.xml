<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="genders" type="java.util.List&lt;com.trainee.facades.product.data.GenderData>"/>
	</bean>

	<bean class="com.trainee.facades.product.data.GenderData">
		<property name="code" type="String"/>
		<property name="name" type="String"/>
	</bean>
	
	<bean class="com.trainee.facades.accenture.AccentureData">
		<property name="id" type="String"/>
		<property name="domain" type="String"/>
		<property name="salary" type="String"/>
	</bean>
	
	<bean class="com.trainee.facades.accenture.AccentureListData">
		<property name="accenture" type="java.util.List&lt;com.trainee.facades.accenture.AccentureData>"/>
	</bean>
	
	<bean class="com.trainee.facades.employee.EmployeeData">
		<property name="id" type="String"/>
		<property name="name" type="String"/>
		<property name="eid" type="String"/>
		<property name="ename" type="String"/>
	</bean>
	
	<bean class="com.trainee.facades.employee.EmployeeListData">
		<property name="employee" type="java.util.List&lt;com.trainee.facades.employee.EmployeeData>"/>
	</bean>
	
		<bean class="com.trainee.facades.tcs.TcsData">
		<property name="id" type="String"/>
		<property name="role" type="String"/>
		<property name="salary" type="String"/>
	</bean>
	
	<bean class="com.trainee.facades.tcs.TcsListData">
		<property name="tcs" type="java.util.List&lt;com.trainee.facades.tcs.TcsData>"/>
	</bean>
	
	<bean class="com.trainee.facades.tcs.TcsData">
		<property name="exp" type="String"/>
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.product.data.ProductData">
		<property name="productSize" type="String"/>
	</bean>
	
	<bean class="de.hybris.platform.commercefacades.user.data.CustomerData">
		<property name="address" type="String"/>
		<property name="pin" type="String"/> 
	</bean>
	
</beans>
